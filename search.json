---
layout: null
---
[
  {% assign pages = site.pages | where_exp:'p','p.url != "/"' %}
  {% for p in pages %}
  {
    "title": "{{ p.title | escape }}",
    "url": "{{ site.baseurl }}{{ p.url }}",
    "content": {{ p.content | strip_html | normalize_whitespace | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
